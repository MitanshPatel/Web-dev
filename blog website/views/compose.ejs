<%- include("partials/header") -%>

<h1>Compose</h1>
<form action="/compose" method="post">
    <label for="title">Title</label>
    <input type="text" name="newTitle" class="form-control" id="title">
    <br>
    <label for="postBlog">Post</label>
    <textarea type="text" name="content" class="form-control" id="postBlog" rows="6"></textarea>
    <br>
    <button type="submit" class="btn btn-primary">Publish</button>
</form>

<%- include("partials/footer") -%>